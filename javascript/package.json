{
  "name": "taiga",
  "version": "0.0.0",
  "license": "MPL-2",
  "scripts": {
    "ng": "nx",
    "postinstall": "node ./decorate-angular-cli.js && ngcc --properties es2015 browser module main",
    "nx": "nx",
    "start": "ng serve",
    "build": "ng build",
    "test": "nx run-many --all --target=test",
    "lint": "nx workspace-lint && npm run lint:angular && npm run lint:html && npm run lint:css",
    "lint:affected": "nx workspace-lint && npm run affected:lint:angular && npm run lint:html && npm run lint:css",
    "lint:angular": "nx run-many --all --target=lint",
    "lint:html": "npx htmlhint 'apps/**/*.html' 'libs/**/*.html'",
    "lint:css": "npx stylelint 'apps/**/*.css' 'libs/**/*.css'",
    "e2e": "ng e2e",
    "affected:lint:angular": "nx run-many --all --target=affected:lint",
    "affected:apps": "nx affected:apps",
    "affected:libs": "nx affected:libs",
    "affected:build": "nx affected:build",
    "affected:e2e": "nx affected:e2e",
    "affected:test": "nx affected:test",
    "affected:lint": "nx affected:lint",
    "affected:dep-graph": "nx affected:dep-graph",
    "affected": "nx affected",
    "format": "nx format:write",
    "format:write": "nx format:write",
    "format:check": "nx format:check",
    "update": "nx migrate latest",
    "workspace-generator": "nx workspace-generator",
    "dep-graph": "nx dep-graph",
    "help": "nx help",
    "dev:ssr": "ng run taiga:serve-ssr",
    "serve:ssr": "node dist/taiga/server/main.js",
    "storybook": "nx run ui:storybook",
    "transloco:optimize": "transloco-optimize dist/taiga/browser/assets/i18n",
    "build:prod": "ng build --prod && npm run transloco:optimize",
    "build:ssr": "npm run build:prod && ng run taiga:server:production",
    "prerender": "ng run taiga:prerender",
    "translations:split": "ng g @ngneat/transloco:split --translation-path 'apps/taiga/src/assets/i18n/'",
    "translations:join": "ng g @ngneat/transloco:join --translation-path 'apps/taiga/src/assets/i18n/'"
  },
  "private": true,
  "husky": {
    "hooks": {
      "pre-commit": "node ./scripts/append-license.js",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS && npm run lint:affected"
    }
  },
  "dependencies": {
    "@angular/animations": "^11.2.0",
    "@angular/cdk": "^11.2.3",
    "@angular/common": "^11.2.0",
    "@angular/compiler": "^11.2.0",
    "@angular/core": "^11.2.0",
    "@angular/forms": "^11.2.0",
    "@angular/platform-browser": "^11.2.0",
    "@angular/platform-browser-dynamic": "^11.2.0",
    "@angular/platform-server": "^11.2.0",
    "@angular/router": "^11.2.0",
    "@ngneat/transloco": "^2.21.0",
    "@ngneat/transloco-messageformat": "^1.3.1",
    "@ngrx/effects": "^11.0.1",
    "@ngrx/router-store": "^11.0.1",
    "@ngrx/store": "^11.0.1",
    "@ngrx/store-devtools": "^11.0.1",
    "@nguniversal/express-engine": "^11.2.0",
    "@nrwl/angular": "12.0.0",
    "@rx-angular/cdk": "^1.0.0-alpha.4",
    "@rx-angular/state": "^1.4.3",
    "@rx-angular/template": "^1.0.0-beta.21",
    "@taiga-ui/cdk": "^2.7.0",
    "@taiga-ui/core": "^2.7.0",
    "@taiga-ui/icons": "^2.7.0",
    "@taiga-ui/kit": "^2.7.0",
    "hotkeys-js": "^3.8.3",
    "immer": "^9.0.1",
    "messageformat": "^2.3.0",
    "ngx-quicklink": "^0.2.7",
    "rxjs": "~6.6.3",
    "tslib": "^2.0.0",
    "zone.js": "^0.10.2"
  },
  "devDependencies": {
    "@angular-builders/custom-webpack": "^11.0.0",
    "@angular-devkit/build-angular": "~0.1102.0",
    "@angular-eslint/eslint-plugin": "2.0.2",
    "@angular-eslint/eslint-plugin-template": "2.0.2",
    "@angular-eslint/template-parser": "2.0.2",
    "@angular/cli": "~11.0.0",
    "@angular/compiler-cli": "^11.2.0",
    "@angular/language-service": "^11.2.0",
    "@commitlint/cli": "^11.0.0",
    "@commitlint/config-conventional": "^11.0.0",
    "@ngneat/reactive-forms": "^1.7.3",
    "@ngneat/spectator": "^7.0.0",
    "@ngneat/transloco-optimize": "^1.0.2",
    "@ngrx/schematics": "^11.0.0",
    "@ngrx/store-devtools": "^11.0.0",
    "@nguniversal/builders": "^11.2.0",
    "@nrwl/cli": "12.0.0",
    "@nrwl/cypress": "12.0.0",
    "@nrwl/eslint-plugin-nx": "12.0.0",
    "@nrwl/jest": "12.0.0",
    "@nrwl/linter": "12.0.0",
    "@nrwl/storybook": "^12.1.0",
    "@nrwl/tao": "12.0.0",
    "@nrwl/workspace": "12.0.0",
    "@storybook/addon-a11y": "^6.2.9",
    "@storybook/addon-actions": "^6.2.9",
    "@storybook/addon-controls": "^6.2.9",
    "@storybook/addon-links": "^6.2.9",
    "@storybook/addon-notes": "^5.3.21",
    "@storybook/angular": "^6.1.11",
    "@types/express": "^4.17.0",
    "@types/faker": "^5.5.6",
    "@types/jest": "26.0.8",
    "@types/node": "14.14.33",
    "@types/webpack": "4.41.21",
    "@typescript-eslint/eslint-plugin": "4.19.0",
    "@typescript-eslint/parser": "4.19.0",
    "cypress": "^6.0.1",
    "dotenv": "8.2.0",
    "eslint": "7.22.0",
    "eslint-config-prettier": "8.1.0",
    "eslint-plugin-cypress": "^2.10.3",
    "eslint-plugin-ngrx": "^1.21.1",
    "express": "^4.15.2",
    "faker": "^5.5.2",
    "htmlhint": "^0.14.2",
    "husky": "^4.3.8",
    "jest": "26.2.2",
    "jest-marbles": "^2.5.1",
    "jest-preset-angular": "8.3.2",
    "ng-mocks": "^11.10.0",
    "ng-packagr": "^11.0.0",
    "postcss-import": "^14.0.0",
    "postcss-mixins": "^7.0.3",
    "postcss-preset-env": "^6.7.0",
    "prettier": "2.2.1",
    "stylelint": "^13.11.0",
    "stylelint-config-standard": "^20.0.0",
    "stylelint-order": "^4.1.0",
    "ts-jest": "26.4.0",
    "ts-node": "~9.1.1",
    "typescript": "4.1.4"
  }
}
