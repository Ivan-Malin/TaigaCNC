{
  "name": "taiga",
  "version": "0.0.0",
  "license": "MPL-2",
  "scripts": {
    "ng": "nx",
    "postinstall": "node ./decorate-angular-cli.js && ngcc --properties es2015 browser module main",
    "nx": "nx",
    "start": "ng serve",
    "build": "ng build && npm run transloco:optimize",
    "default-config": "cp config.example.json apps/taiga/src/assets/config.json",
    "test": "nx run-many --all --target=test",
    "lint": "nx workspace-lint && npm run lint:angular && npm run lint:html && npm run lint:css",
    "lint:affected": "nx workspace-lint && npm run affected:lint:angular && npm run lint:html && npm run lint:css",
    "lint:angular": "nx run-many --all --target=lint",
    "lint:html": "npx htmlhint 'apps/**/*.html' 'libs/**/*.html'",
    "lint:css": "npx stylelint 'apps/**/*.css' 'libs/**/*.css'",
    "e2e": "nx e2e taiga-e2e",
    "e2e:admin": "nx e2e taiga-e2e --watch",
    "affected:lint:angular": "nx run-many --all --target=affected:lint",
    "affected:apps": "nx affected:apps",
    "affected:libs": "nx affected:libs",
    "affected:build": "nx affected:build",
    "affected:e2e": "nx affected:e2e",
    "affected:test": "nx affected:test",
    "affected:lint": "nx affected:lint",
    "affected:dep-graph": "nx affected:dep-graph",
    "affected": "nx affected",
    "format": "nx format:write",
    "format:write": "nx format:write",
    "format:write:all": "nx format:write --all",
    "format:check": "nx format:check",
    "update": "nx migrate latest",
    "workspace-generator": "nx workspace-generator",
    "dep-graph": "nx dep-graph",
    "help": "nx help",
    "dev:ssr": "ng run taiga:serve-ssr",
    "serve:ssr": "node dist/taiga/server/main.js",
    "storybook": "nx run ui:storybook",
    "transloco:optimize": "transloco-optimize dist/taiga/browser/assets/i18n",
    "build:prod": "ng build --prod && npm run transloco:optimize",
    "build:ssr": "npm run build:prod && ng run taiga:server:production",
    "prerender": "ng run taiga:prerender",
    "translations:split": "ng g @ngneat/transloco:split --translation-path 'apps/taiga/src/assets/i18n/'",
    "translations:join": "ng g @ngneat/transloco:join --translation-path 'apps/taiga/src/assets/i18n/'",
    "install:hooks": "cd .. && husky install javascript/.husky",
    "prepare": "npm run install:hooks",
    "emails:compile-html": "npx mjml --config.keepComments=0 ./apps/taiga-mjml/templates/*.mjml.html.jinja -o ../python/apps/taiga/src/taiga/emails/templates",
    "emails:copy-text": "npx copyfiles -f ./apps/taiga-mjml/templates/*.txt.jinja ../python/apps/taiga/src/taiga/emails/templates",
    "emails:copy-subject": "npx copyfiles -f ./apps/taiga-mjml/templates/*.subject.jinja ../python/apps/taiga/src/taiga/emails/templates",
    "emails": "npm run emails:compile-html && npm run emails:copy-text && npm run emails:copy-subject && node ./scripts/append-license.js"
  },
  "lint-staged": {
    "*.js": "eslint --cache --fix",
    "*.css": "stylelint --fix"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "13.2.0",
    "@angular/cdk": "13.2.0",
    "@angular/common": "13.2.0",
    "@angular/compiler": "13.2.0",
    "@angular/core": "13.2.0",
    "@angular/forms": "13.2.0",
    "@angular/platform-browser": "13.2.0",
    "@angular/platform-browser-dynamic": "13.2.0",
    "@angular/platform-server": "13.2.0",
    "@angular/router": "13.2.0",
    "@ngneat/transloco": "^3.1.3",
    "@ngneat/transloco-messageformat": "^3.0.1",
    "@ngneat/until-destroy": "^9.0.0",
    "@ngrx/effects": "13.0.2",
    "@ngrx/router-store": "13.0.2",
    "@ngrx/store": "13.0.2",
    "@ngrx/store-devtools": "13.0.2",
    "@nguniversal/express-engine": "^13.0.2",
    "@nrwl/angular": "13.7.1",
    "@rx-angular/cdk": "^1.0.0-alpha.11",
    "@rx-angular/state": "^1.4.6",
    "@rx-angular/template": "^1.0.0-beta.29",
    "@taiga-ui/addon-table": "^2.31.0",
    "@taiga-ui/addon-tablebars": "^2.31.0",
    "@taiga-ui/cdk": "^2.31.0",
    "@taiga-ui/core": "^2.31.0",
    "@taiga-ui/icons": "^2.31.0",
    "@taiga-ui/kit": "^2.31.0",
    "angular-resize-event": "^3.1.1",
    "change-case": "^4.1.2",
    "eslint-plugin-taiga": "file:eslint-plugin-taiga",
    "hotkeys-js": "^3.8.7",
    "immer": "^9.0.12",
    "messageformat": "^2.3.0",
    "ngx-quicklink": "^0.2.7",
    "rxjs": "^6.6.7",
    "tslib": "^2.3.1",
    "zone.js": "0.11.4"
  },
  "devDependencies": {
    "@angular-builders/custom-webpack": "^13.0.0",
    "@angular-devkit/build-angular": "13.2.1",
    "@angular-devkit/schematics-cli": "13.2.1",
    "@angular-eslint/eslint-plugin": "13.0.1",
    "@angular-eslint/eslint-plugin-template": "13.0.1",
    "@angular-eslint/template-parser": "13.0.1",
    "@angular/cli": "13.2.1",
    "@angular/compiler-cli": "13.2.0",
    "@angular/language-service": "13.2.0",
    "@commitlint/cli": "^16.1.0",
    "@commitlint/config-conventional": "^16.0.0",
    "@ng-web-apis/universal": "^1.12.0",
    "@ngneat/falso": "^2.14.0",
    "@ngneat/spectator": "^10.0.0",
    "@ngneat/transloco-optimize": "^3.0.2",
    "@ngrx/schematics": "13.0.2",
    "@ngrx/store-devtools": "13.0.2",
    "@nguniversal/builders": "^13.0.2",
    "@nrwl/cli": "13.7.1",
    "@nrwl/cypress": "13.7.1",
    "@nrwl/eslint-plugin-nx": "13.7.1",
    "@nrwl/jest": "13.7.1",
    "@nrwl/linter": "13.7.1",
    "@nrwl/storybook": "13.7.1",
    "@nrwl/tao": "13.7.1",
    "@nrwl/workspace": "13.7.1",
    "@storybook/addon-a11y": "^6.4.14",
    "@storybook/addon-actions": "^6.4.14",
    "@storybook/addon-controls": "~6.4.14",
    "@storybook/addon-essentials": "6.4.14",
    "@storybook/addon-links": "^6.4.14",
    "@storybook/addon-notes": "^5.3.21",
    "@storybook/angular": "6.4.14",
    "@storybook/builder-webpack5": "6.4.14",
    "@storybook/manager-webpack5": "6.4.14",
    "@types/express": "^4.17.13",
    "@types/faker": "^6.6.8",
    "@types/jest": "27.4.0",
    "@types/node": "17.0.10",
    "@types/webpack": "5.28.0",
    "@typescript-eslint/eslint-plugin": "5.10.2",
    "@typescript-eslint/parser": "5.10.2",
    "axe-core": "^4.3.5",
    "copyfiles": "^2.4.1",
    "cypress": "^9.3.1",
    "cypress-axe": "^0.14.0",
    "dotenv": "14.2.0",
    "eslint": "8.7.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-cypress": "^2.12.1",
    "eslint-plugin-ngrx": "^2.0.3",
    "express": "^4.17.2",
    "faker": "^6.6.6",
    "htmlhint": "^1.1.1",
    "husky": "^7.0.4",
    "jest": "27.4.7",
    "jest-marbles": "^3.0.2",
    "jest-preset-angular": "11.1.0",
    "lint-staged": "^12.3.1",
    "mjml": "^4.12.0",
    "ng-mocks": "^13.0.0",
    "ng-packagr": "13.2.1",
    "postcss": "^8.4.5",
    "postcss-import": "^14.0.2",
    "postcss-loader": "^6.2.1",
    "postcss-mixins": "^9.0.1",
    "postcss-preset-env": "^7.2.3",
    "postcss-url": "^10.1.3",
    "prettier": "^2.5.1",
    "schematics-utilities": "^2.0.3",
    "stylelint": "^14.3.0",
    "stylelint-config-prettier": "^9.0.3",
    "stylelint-config-standard": "^24.0.0",
    "stylelint-order": "^5.0.0",
    "stylelint-use-logical-spec": "^3.2.2",
    "ts-jest": "27.1.3",
    "ts-node": "~10.4.0",
    "type-fest": "^2.10.0",
    "typescript": "4.5.5"
  }
}
